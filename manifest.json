{
  "manifest_version": 3,
  "name": "Notification Opener",
  "version": "0.1",
  "description": "Open notification links in new tabs",
  "permissions": ["activeTab", "tabs", "scripting"],
  "action": {
      "default_icon": "assets/notifications.png"
  },
  "background": {
      "service_worker": "background.js"
  },
  "icons": {
      "48": "assets/notifications.png"
  }
}